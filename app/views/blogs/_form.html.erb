<%= simple_form_for(@blog) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :url %>
    <%= f.input :description %>
    <%= f.association :channel %>
    <%= f.association :language %>
    <%= f.input :avg_mon_imps %>
    <%= f.input :alexa %>
    <%= f.input :page_rank %>
    <%= f.input :desired_price %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
